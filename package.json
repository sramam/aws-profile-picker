{
  "name": "aws-profile-picker",
  "version": "1.0.5",
  "description": "interactive AWS profile picker",
  "keywords": [
    "aws",
    "profile",
    "picker"
  ],
  "author": "Shishir Ramam",
  "repository": "sramam/aws-profile-picker",
  "license": "Apache-2.0",
  "private": false,
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "scripts"
  ],
  "scripts": {
    "staged-mods-ok": "git-mods --staged_ok",
    "no-mods": "git-mods",
    "clean": "rimraf build coverage",
    "clean:dist": "rimraf dist",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "compile:build": "tsc",
    "compile:dist": "tsc -p tsconfig.dist.json",
    "compile:watch": "tsc -w",
    "sh": "npm-sh",
    "secure": "nsp check",
    "check": "ncu -e 1",
    "test-with-cover": "istanbul cover ./node_modules/.bin/_mocha",
    "test-without-cover": "mocha",
    "remap-coverage:dist": "node ./scripts/remapped-coverage.js --force_min_cover true --output ./coverage/typescript",
    "remap-coverage": "node ./scripts/remapped-coverage.js --force_min_cover=false --output ./coverage/typescript",
    "coverage": "istanbul check-coverage ./coverage/typescript/coverage.json",
    "combine-coverage": "istanbul-combine -d coverage -p summary -r lcov -r html coverage/partials/**/*.",
    "build": "npm-run-batch",
    "build:dist": "run-batch",
    "build:watch": "cross-env NODE_ENV=production run-batch",
    "test": "run-batch",
    "test:dist": "run-batch",
    "pkg-ok": "pkg-ok",
    "precommit": "run-batch",
    "prepush": "run-batch",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "run-batch": {
    "build": [
      "clean",
      "lint",
      "compile:build",
      "test-without-cover"
    ],
    "build:watch": [
      "compile:watch"
    ],
    "build:dist": [
      "lint",
      "clean",
      "compile:build",
      "test:dist",
      "clean:dist",
      "compile:dist",
      "pkg-ok"
    ],
    "test:dist": [
      "test-without-cover"
    ],
    "test": [
      "test-without-cover"
    ],
    "precommit": [
      "postinstall",
      "build",
      "staged-mods-ok"
    ],
    "prepush": [
      "build:dist",
      "check",
      "secure",
      "no-mods"
    ]
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/circular-json": "^0.1.30",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "@types/semver": "^5.3.31",
    "chai": "^3.5.0",
    "cli-inspector": "^1.0.3",
    "coveralls": "^2.12.0",
    "cross-env": "^3.2.4",
    "git-mods": "^1.0.4",
    "graceful-fs": "^4.1.11",
    "husky": "^0.13.2",
    "istanbul": "^0.4.5",
    "istanbul-combine": "^0.3.0",
    "mocha": "^3.2.0",
    "node-emoji": "^1.5.1",
    "npm-check-updates": "^2.10.3",
    "npm-run-batch": "0.0.4",
    "npm-sh": "^1.0.1",
    "nsp": "^2.6.3",
    "nsp-formatter-checkstyle": "^1.0.1",
    "nsp-formatter-remarkup": "^1.0.2",
    "parse-git-config": "^1.1.1",
    "pkg-ok": "^1.0.1",
    "remap-istanbul": "^0.9.1",
    "rimraf": "^2.6.1",
    "shelljs-nodecli": "^0.1.1",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "commander": "^2.9.0",
    "expand-home-dir": "0.0.3",
    "ini": "^1.3.4",
    "inquirer": "^3.1.1",
    "runtime-engine-check": "^1.0.0",
    "semver": "^5.3.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
